FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      turbulenceProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

simulationType RAS;

RAS
{
    RASModel            kEpsilon;
    turbulence          on;
    printCoeffs         on;
    kEpsilonCoeffs
    {
        C1    1.6;
    }
}

