FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      TTCov;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 0;

boundaryField
{
    jet
    {
        type            fixedValue;
        value           uniform 0;
    }
    pilot
    {
        type            fixedValue;
        value           uniform 0;
    }
    coflow
    {
        type            fixedValue;
        value           uniform 0;
    }
    outflow
    {
        type            zeroGradient;
    }
    axis
    {
        type            zeroGradient;
    }
    outerWall
    {
        type            zeroGradient;
    }
    back
    {
        type            wedge;
    }
    front
    {
        type            wedge;
    }
}

