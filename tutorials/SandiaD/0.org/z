FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0";
    object      z;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    front
    {
        type            wedge;
    }
    back
    {
        type            wedge;
    }
    jet
    {
        type            fixedValue;
        value           uniform 1;
    }
    pilot
    {
        type            fixedValue;
        value           uniform 0.268692;
    }
    coflow
    {
        type            fixedValue;
        value           uniform 0;
    }
    outerWall
    {
        type            zeroGradient;
    }
    outflow
    {
        type            zeroGradient;
    }
    axis
    {
        type            zeroGradient;
    }
}

