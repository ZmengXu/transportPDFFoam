FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue z 0
);

regions
(
    boxToCell
    {
        box (-100 1 -100) (100 100 100);

        fieldValues
        (
            volScalarFieldValue z 1
        );
    }
);

